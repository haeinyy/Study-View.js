-col>\n        <b-table\n          striped\n          hover\n          :items=\"articles\"\n          :fields=\"fields\"\n          @row-clicked=\"viewArticle\"\n        >\n        </b-table>\n      </b-col>\n    </b-row>\n  </b-container>\n</template>\n\n<script>\nimport http from \"@/util/http-common\";\n// import BoardListRow from \"@/components/board/BoardListRow\";\n\nexport default {\n  name: \"BoardList\",\n  components: {\n    // BoardListRow,\n  },\n  data() {\n    return {\n      articles: [],\n      fields: [\n        { key: \"no\", label: \"글번호\", tdClass: \"tdClass\" },\n        { key: \"title\", label: \"제목\", tdClass: \"tdSubject\" },\n        { key: \"writer\", label: \"작성자\", tdClass: \"tdClass\" },\n        { key: \"regtime\", label: \"작성일\", tdClass: \"tdClass\" },\n      ],\n    };\n  },\n  created() {\n    http.get(`/api/board`).then(({ data }) => {\n      this.articles = data;\n    });\n  },\n  methods: {\n    moveWrite() {\n      this.$router.push({ name: \"BoardWrite\" });\n    },\n    viewArticle(article) {\n      this.$router.push({ name: \"BoardView\", params: { no: article.no } });\n    },\n  },\n};\n</script>\n\n<style scope>\n.tdClass {\n  width: 50px;\n  text-align: center;\n}\n.tdSubject {\n  width: 300px;\n  text-align: left;\n}\n</style>\n"]}]}